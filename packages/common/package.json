{
  "name": "@nuralyui/common",
  "version": "0.1.1",
  "description": "Common UI components for Nuraly UI Library - includes basic building blocks like Icon, Badge, Divider, and Label",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./mixins": {
      "import": "./dist/mixins/index.js",
      "types": "./dist/mixins/index.d.ts"
    },
    "./controllers": {
      "import": "./dist/controllers/index.js",
      "types": "./dist/controllers/index.d.ts"
    },
    "./themes": {
      "import": "./dist/themes/index.js",
      "types": "./dist/themes/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "VERSIONS.md"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "clean": "rm -rf dist/*",
    "versions": "node scripts/versions.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "ui-components",
    "icon",
    "badge",
    "divider",
    "label",
    "common",
    "nuralyui"
  ],
  "author": "Laabidi Aymen",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nuralyio/NuralyUI.git",
    "directory": "packages/common"
  },
  "bugs": {
    "url": "https://github.com/Nuralyio/NuralyUI/issues"
  },
  "homepage": "https://github.com/Nuralyio/NuralyUI#readme",
  "peerDependencies": {
    "lit": "^2.2.4",
    "@lit/reactive-element": "^1.6.1"
  },
  "devDependencies": {
    "fs-extra": "^11.1.1"
  }
}
